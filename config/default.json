{
  "datafile":"history.db",
  "userfile":"users.db",
  "syncfile":"sync.db",
  "salt":"my_first_bookmark_service",
  "serverConfig":{
    "host":"",
    "port":8443
  },
  "session":{
    "name":"bookmark_service",
    "secret":"bookmark_service",
    "sessionTimeOut":31536000000
  },
  "loginConfig": {
    "loginSuccessRedirect": "/browse.html",
