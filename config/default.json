    "loginPage": "login/login_app",
    "noLoginPathStartPathList":["/login","/login.html","/create","/js","/css","/check_login","/bookmarksapp.manifest"],
    "noLoginExactMatch":["/bookmarksapp.manifest"]
  }
}
